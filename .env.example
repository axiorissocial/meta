# Docker Compose Environment Variables
# Copy this file to .env and update with your values

# Secrets (IMPORTANT: Change these in production!)
SESSION_SECRET=your-secret-key-change-this
MESSAGE_ENCRYPTION_KEY=change-this-encryption-key

# GitHub OAuth Integration
GITHUB_CLIENT_ID=CHANGE_THIS
GITHUB_CLIENT_SECRET=CHANGE_THIS

# Google OAuth Integration (optional)
GOOGLE_CLIENT_ID=CHANGE_THIS
GOOGLE_CLIENT_SECRET=CHANGE_THIS
GOOGLE_CALLBACK_URL=http://localhost:5173/api/auth/google/callback

# Environment
NODE_ENV=development
